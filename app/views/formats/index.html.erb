<h1>Listing formats</h1>

<table>
  <tr>
    <th>Imput</th>
    <th>Output</th>
    <th>Material</th>
    <th>Tool</th>
    <th>Operation</th>
    <th>Caution</th>
    <th>Intention</th>
    <th>Customize</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @formats.each do |format| %>
  <tr>
    <td><%= format.imput %></td>
    <td><%= format.output %></td>
    <td><%= format.material %></td>
    <td><%= format.tool %></td>
    <td><%= format.operation %></td>
    <td><%= format.caution %></td>
    <td><%= format.intention %></td>
    <td><%= format.customize %></td>
    <td><%= link_to 'Show', format %></td>
    <td><%= link_to 'Edit', edit_format_path(format) %></td>
    <td><%= link_to 'Destroy', format, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Format', new_format_path %>
